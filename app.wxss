/**app.wxss**/
@import 'weui/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 
.common-box{
  background: #f5f5f5;
  padding-top:20rpx;
}
.main-content{
  width:100%;
  background: #fff;
}
view,image,text,navigator{
  box-sizing: border-box;
  padding:0;
  margin:0;

}
view,text{
  font-family: PingFangSC-Light,helvetica,'Heiti SC';
  font-size: 32rpx;
  color: #333;
}
