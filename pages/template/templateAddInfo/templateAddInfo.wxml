
<view class="container common-box">
  <view class="main-content">
      <view class="form-info-list">
        <view wx:for="{{renderLidst}}" wx:key="{{item.name}}" class="form-item">
            <label>{{item.name}}</label>
            <input  class="userame" value="{{item.value}}" data-key="{{item.name}}"  bindinput="bindUsernameInput" placeholder="{{'请输入'}}"></input>
        </view>
        <view class="line"></view>
      </view>
      <button class='contact-setting' bindtap='signSetting'>参数提交</button>
  </view>
</view>
