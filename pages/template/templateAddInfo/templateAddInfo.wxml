
<view class="container common-box">
  <view class="main-content">
      <view class="form-info-list">
        <view wx:for="{{infoList}}" wx:key="{{item}}" class="form-item">
            <label>{{item}}</label>
            <input  class="userame" value="{{fillVal}}" data-key="{{item}}" bindinput="bindUsernameInput" placeholder="{{'请输入'}}"></input>
        </view>
        <view class="line"></view>
      </view>
      <button class='contact-setting' bindtap='signSetting'>签署设置</button>
  </view>
</view>
