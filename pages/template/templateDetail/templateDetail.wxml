<view class='containter common-box'>
  <view class='main-content'>
    <view class='contract-img'>
      <image src='{{baseUrl}}/restapi/wesign/v1/tenant/contract/img?contractUrl={{item}}' wx:for='{{contractList}}' wx:key="{{item}}">{{item}}</image>
      <view wx:for='{{contractList}}' wx:key="{{item}}">{{item}}</view>
    </view>
    <button class='contact-start' bindtap='ImmediatelyStart'>立即发起</button>
  </view>
</view>