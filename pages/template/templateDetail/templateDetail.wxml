<view class='containter common-box'>
  <view class='main-content'>
    <view class='contract-img'>
      <image style="height:{{imgHeight}}px" src='{{baseUrl}}/restapi/wesign/v1/tenant/contract/img?contractUrl={{item}}' wx:for='{{contractList}}' wx:key="{{item}}">{{item}}</image>
    </view>
    <button class='contact-start' v-if="{{templateSpecificType == 'fillreference'}}" bindtap='ImmediatelyStart'>立即发起</button>
  </view>
</view>